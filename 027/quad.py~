import math

def checkprime(a):
    if a == 2:
        return True
    for i in range(2,math.ceil(math.sqrt(a))+1):
        if a%i == 0:
            return False
    return True

def formula(n,a,b):
    return n**2 + a*n + b

for a,b in range(1000),range(1000):
    for i in range(10):
        if checkprime(formula(n,a,b)):
            print(a,b)
