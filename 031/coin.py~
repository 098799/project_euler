denominaly=[200,100,50,20,10,5,2,1]

def rozmien(a):
    cont = 0
    if a == 0:
        cont += 1
    if a == 1:
        reszta = a - 1
        rozmien(reszta)
    if a > 1:
        for i in range(len(denominaly)):
            if a >= denominaly[i]:
                reszta = a - denominaly[i]
    return cont
    
for i in range(0,10):
    print(i,rozmien(i))
